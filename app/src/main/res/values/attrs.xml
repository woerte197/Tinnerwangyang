<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="textSize" format="dimension" />
    <attr name="imgWidth" format="dimension" />
    <attr name="imgHeight" format="dimension" />
    <attr name="textValue" format="string" />
    <attr name="showPop" format="boolean" />
    <attr name="selecterDisable" format="boolean" />
    <attr name="imgHead" format="reference" />
    <declare-styleable name="InterestPlugin">
        <attr name="wordText" format="string" />
    </declare-styleable>
    <declare-styleable name="TabItem">
        <attr name="textSize" />
        <attr name="imgWidth" />
        <attr name="imgHeight" />
        <attr name="textValue" />
        <attr name="showPop" />
        <attr name="selecterDisable" />
        <attr name="imgHead" />
    </declare-styleable>
    <declare-styleable name="TabBar">
        <attr name="textSize" />
        <attr name="imgWidth" />
        <attr name="imgHeight" />
        <attr name="textValue" />
        <attr name="showPop" />
        <attr name="selecterDisable" />
        <attr name="imgHead" />
    </declare-styleable>


    <attr name="fromDegrees" format="float|reference" />
    <attr name="toDegrees" format="float|reference" />
    <attr name="childSize" format="dimension|reference" />

    <attr name="centered" format="boolean" />
    <attr name="selectedColor" format="color" />
    <attr name="strokeWidth" format="dimension" />
    <attr name="unselectedColor" format="color" />

    <attr name="sriv_left_top_corner_radius" format="dimension" />
    <attr name="sriv_right_top_corner_radius" format="dimension" />
    <attr name="sriv_left_bottom_corner_radius" format="dimension" />
    <attr name="sriv_right_bottom_corner_radius" format="dimension" />
    <attr name="sriv_border_width" format="dimension" />
    <attr name="sriv_border_color" format="color" />
    <attr name="sriv_oval" format="boolean" />

    <declare-styleable name="SelectableRoundedImageView">
        <attr name="sriv_left_top_corner_radius" />
        <attr name="sriv_right_top_corner_radius" />
        <attr name="sriv_left_bottom_corner_radius" />
        <attr name="sriv_right_bottom_corner_radius" />
        <attr name="sriv_border_width" />
        <attr name="sriv_border_color" />
        <attr name="sriv_oval" />
        <attr name="android:scaleType" />
    </declare-styleable>
    <attr name="slide_first_text" format="string" />
    <attr name="slide_text_color" format="color" />
    <declare-styleable name="Sidebar">
        <attr name="slide_first_text" />
        <attr name="slide_text_color" />
    </declare-styleable>
    <declare-styleable name="CalendarView">
        <!--是否显示农历-->
        <attr name="show_lunar" format="boolean" />
        <!--是否显示上月和下月-->
        <attr name="show_last_next" format="boolean" />
        <!--是否显示节假日-->
        <attr name="show_holiday" format="boolean" />
        <!--是否显示节气-->
        <attr name="show_term" format="boolean" />
        <!--阳历的日期颜色-->
        <attr name="solar_color" format="color" />
        <!--阳历的日期尺寸-->
        <attr name="solar_size" format="integer" />
        <!--农历的日期颜色-->
        <attr name="lunar_color" format="color" />
        <!--农历的日期尺寸-->
        <attr name="lunar_size" format="integer" />
        <!--节日文字颜色-->
        <attr name="holiday_color" format="color" />
        <!--选中的日期文字颜色-->
        <attr name="choose_color" format="color" />
        <!--选中的日期背景-->
        <attr name="day_bg" format="reference" />
        <!--单选时切换月份，是否选中上次的日期-->
        <attr name="switch_choose" format="boolean" />
        <!--设置单选、多选-->
        <attr name="choose_type" format="enum">
            <enum name="single" value="0" />
            <enum name="multi" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="WeekView">
        <!--周的表示形式（x.x.x.x.x.x.x）-->
        <attr name="week_str" format="string" />
        <!--周的文字颜色-->
        <attr name="week_color" format="color" />
        <!--周的文字大小-->
        <attr name="week_size" format="integer" />
    </declare-styleable>
    <declare-styleable name="SlantedTextView">
        <attr name="slantedTextSize" format="dimension" />
        <attr name="slantedBackgroundColor" format="color" />
        <attr name="slantedText" format="string" />
        <attr name="slantedTextColor" format="color" />
        <attr name="slantedLength" format="dimension" />
        <attr name="slantedMode" format="enum">
            <enum name="left" value="0"></enum>
            <enum name="right" value="1"></enum>
            <enum name="left_bottom" value="2"></enum>
            <enum name="right_bottom" value="3"></enum>
            <enum name="left_triangle" value="4"></enum>
            <enum name="right_triangle" value="5"></enum>
            <enum name="left_bottom_triangle" value="6"></enum>
            <enum name="right_bottom_triangle" value="7"></enum>
        </attr>
    </declare-styleable>
</resources>
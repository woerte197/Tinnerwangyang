<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="oval">
            <solid android:color="#f6f6f6" />
        </shape>
    </item>
    <item     >
        <shape android:shape="oval">
            <stroke
                android:width="1.5dp"
                android:color="#ccc" />

        </shape>
    </item>
    <item  android:right="2dp"  >
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="#f6f6f6" />

        </shape>
    </item>
</layer-list>